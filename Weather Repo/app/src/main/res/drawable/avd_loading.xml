<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="340dp"
            android:height="246dp"
            android:viewportWidth="340"
            android:viewportHeight="246">
            <group
                android:name="group"
                android:pivotX="170"
                android:pivotY="123"
                android:rotation="50">
                <path
                    android:name="path"
                    android:pathData="M 170 241 C 104.83 241 52 188.17 52 123 C 52 57.83 104.83 5 170 5 C 235.17 5 288 57.83 288 123 C 288 188.17 235.17 241 170 241 Z"
                    android:strokeColor="@color/black15F2"
                    android:strokeWidth="10"
                    android:strokeLineCap="round"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:duration="822"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="trimPathEnd"
                    android:startOffset="82"
                    android:duration="822"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="712"
                    android:duration="288"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:duration="258"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
